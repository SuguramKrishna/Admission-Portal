<%= form_with url: "/staff_add", method: :post do |form| %>
  <%= form.text_field "staff[name]", placeholder: "name"%>
  <%= form.text_field "staff[designation]", placeholder:"designation"%>
  <%= form.text_field "staff[experience]", placeholder: "experience"%>
  <%= form.text_field "staff[email_id]", placeholder: "email_id"%>
  <%= form.number_field "staff[age]", placeholder: "age"%>
  <%= form.number_field "staff[phone_number]", placeholder: "number"%>
  <%= form.file_field "aadhar_card_images", multiple: true %>
  <%= form.submit "Submit" %>
<% end %>